height = float(input("Enter Your Height in meter :"))
weight = float(input("Enter Your Weight in kg :"))

BMI = weight / (height ** 2)
if( 18.5 <= BMI <= 24.9):
    print("healthy")
elif(25 <= BMI <= 29.9):
    print("overweight")
elif(BMI < 18.5):
    print("underweight")
elif(BMI > 18.5):
    print("obesity")

print("Your BMI is :",BMI)
